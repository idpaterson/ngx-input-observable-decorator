{
  "name": "ngx-input-observable-decorator",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build-lib",
    "test": "ng test --watch",
    "travis": "ng build ngx-input-observable-decorator && ng test ngxInputObservableDecorator --browsers Firefox",
    "watch-lib": "ng build ngx-input-observable-decorator --watch",
    "build-lib": "npm run copy-readme-to-lib-folder && ng build ngx-input-observable-decorator",
    "build-lib-prod": "npm run copy-readme-to-lib-folder && ng build ngx-input-observable-decorator --prod",
    "test-lib": "ng test ngx-input-observable-decorator",
    "test-lib-ci": "ng test ngx-input-observable-decorator --watch=false --browsers=ChromeHeadless",
    "lint-lib": "ng lint ngx-input-observable-decorator",
    "pack-lib": "npm run build-lib-prod && npm pack ./dist/ngx-input-observable-decorator",
    "publish-lib": "npm run build-lib-prod && npm publish ./dist/ngx-input-observable-decorator",
    "publish-lib-next": "npm run build-lib && npm publish --tag next ./dist/ngx-input-observable-decorator",
    "copy-readme-to-lib-folder": "copyfiles README.md ./projects/ngx-input-observable-decorator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.3",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/forms": "10.1.3",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/router": "10.1.3",
    "core-js": "3.6.5",
    "rxjs": "6.6.3",
    "tslib": "2.0.1",
    "zone.js": "0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular-devkit/build-ng-packagr": "0.1001.3",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.3",
    "@angular/language-service": "10.1.3",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "14.11.2",
    "copyfiles": "2.3.0",
    "codelyzer": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "10.1.2",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "3.9.5"
  }
}
